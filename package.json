{
  "name": "world-clocks",
  "version": "1.0.0",
  "description": "Displays the time from around the world",
  "private": true,
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --progress --config webpack.config.dev.js",
    "build:prod": "webpack --progress --config webpack.config.prod.js",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --progress --config webpack.config.dev.js",
    "test": "jest",
    "watch": "webpack --progress --colors --watch --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desktop-examples/world-clocks.git"
  },
  "keywords": [
    "example",
    "clocks"
  ],
  "author": "ReactiveMarkets",
  "license": "ISC",
  "dependencies": {
    "debug": "^4.1.1",
    "mobx": "^5.9.4",
    "mobx-react": "^6.1.3",
    "moment-timezone": "^0.5.23",
    "react": "^16.8.6",
    "react-dom": "^16.9.0",
    "react-flexview": "^4.0.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.11",
    "@types/moment-timezone": "^0.5.9",
    "@types/node": "^11.13.4",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.4",
    "autoprefixer": "^9.6.1",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-inline-css-webpack-plugin": "^1.3.2",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.8.0",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.9.0",
    "terser-webpack-plugin": "^1.4.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.0",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1"
  }
}
