{
  "name": "world-clocks",
  "version": "1.0.0",
  "description": "Displays the time from around the world",
  "private": true,
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --progress --config webpack.config.dev.js",
    "build:prod": "webpack --progress --config webpack.config.prod.js",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "start": "webpack-dev-server --progress --config webpack.config.dev.js",
    "watch": "webpack --progress --colors --watch --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desktop-examples/world-clocks.git"
  },
  "keywords": [
    "example",
    "clocks"
  ],
  "author": "Reactive Markets",
  "license": "Apache",
  "dependencies": {
    "debug": "^4.3.2",
    "mobx": "^5.15.7",
    "mobx-react": "^6.3.1",
    "moment-timezone": "^0.5.33",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-flexview": "^6.0.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^16.4.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "autoprefixer": "^9.8.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.2.7",
    "file-loader": "^6.2.0",
    "html-inline-css-webpack-plugin": "^1.11.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "mini-css-extract-plugin": "^1.6.2",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.3.0",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.3.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.2"
  }
}
