{
  "name": "world-clocks",
  "version": "1.0.0",
  "description": "Displays the time from around the world",
  "private": true,
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --progress --config webpack.config.dev.js",
    "build:prod": "webpack --progress --config webpack.config.prod.js",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "start": "webpack-dev-server --progress --config webpack.config.dev.js",
    "watch": "webpack --progress --colors --watch --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desktop-examples/world-clocks.git"
  },
  "keywords": [
    "example",
    "clocks"
  ],
  "author": "Reactive Markets",
  "license": "Apache",
  "dependencies": {
    "debug": "^4.1.1",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "moment-timezone": "^0.5.31",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-flexview": "^5.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^9.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-inline-css-webpack-plugin": "^1.8.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "terser-webpack-plugin": "^3.0.3",
    "ts-loader": "^7.0.5",
    "tslint": "^6.1.2",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
