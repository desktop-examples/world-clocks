{
  "name": "world-clocks",
  "version": "1.0.0",
  "description": "Displays the time from around the world",
  "private": true,
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --progress --config webpack.config.dev.js",
    "build:prod": "webpack --progress --config webpack.config.prod.js",
    "lint": "tslint -p tsconfig.json -c tslint.yaml",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --progress --config webpack.config.dev.js",
    "test": "jest",
    "watch": "webpack --progress --colors --watch --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desktop-examples/world-clocks.git"
  },
  "keywords": [
    "example",
    "clocks"
  ],
  "author": "ReactiveMarkets",
  "license": "ISC",
  "dependencies": {
    "debug": "^4.1.1",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.6",
    "moment-timezone": "^0.5.27",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-flexview": "^4.0.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.1.1",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "autoprefixer": "^9.7.4",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "html-inline-css-webpack-plugin": "^1.8.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "offline-plugin": "^5.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.12.0",
    "terser-webpack-plugin": "^2.3.4",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.7.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
